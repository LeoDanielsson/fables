<div class="row">
    {{#each this}}
        <div class="col s12 m6">
            <div class="card large blue-grey lighten-5">
                <div class="card-image">
                    <img src="{{imageUrl}}">
                    <span class="card-title">{{name}} av {{artist}}</span>
                </div>
                <div class="card-content">
                    <p>{{description}}</p>
                </div>
                <div class="card-action">
                    <a class="blue-grey-text"href="#!/fable/{{id}}">Lyssna på berättelsen</a>
                </div>
            </div>
        </div>
    {{/each}}
</div>
